{
  "name": "react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "build:prod": "react-router build",
    "build": "react-router build && mkdir -p dist && mv build/client/* dist/ && rm -rf build",
    "build:dev": "react-router build --mode development",
    "dev": "react-router dev",
